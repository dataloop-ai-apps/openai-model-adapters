{
  "name": "text-embeddings",
  "displayName": "Text Embeddings",
  "version": "0.0.9",
  "scope": "public",
  "description": "Text Embeddings 3 Model Application by Open AI",
  "attributes": {
    "Provider": "Open AI",
    "Category": "Model",
    "NLP": "Feature extraction",
    "Media Type": "Text"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/openai-model-adapters.git",
    "gitTag": "0.0.9"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "text-embeddings-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "python:3.10",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "text-embeddings-module",
        "entryPoint": "adapters/text_embeddings/text_embeddings.py",
        "className": "ModelAdapter",
        "computeConfig": "text-embeddings-deploy",
        "description": "Text Embeddings 3 Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          },
          {
            "type": "String",
            "name": "openai_key_name"
          }
        ],
        "functions": [
          {
            "name": "extract_items_features",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input items for prediction."
              },
              {
                "type": "Feature[]",
                "name": "feature",
                "description": "The extracted features."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Text Embeddings 3 API"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
            ],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Text Embeddings 3 Open AI Dataset"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "openai-gpt-3.5-turbo",
        "moduleName": "text-embeddings-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {},
        "description": "OpenAI API call for gpt-3.5-turbo"
      }
    ]
  }
}